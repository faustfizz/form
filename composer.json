{
    "name": "palmtree/form",
    "description": "Form builder with bootstrap classes, validation, Recaptcha support, AJAX submissions and more",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andy Palmer",
            "email": "andy@andypalmer.me",
            "homepage": "https://andypalmer.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "Palmtree\\Form\\": "src"
        }
    },
    "require": {
        "php": ">=7.1",
        "palmtree/argparser": "^2.1",
        "palmtree/nameconverter": "^2.0",
        "palmtree/html": "^3.0"
    },
    "require-dev": {
        "palmtree/php-cs-fixer-config": "^1.1",
        "vimeo/psalm": "^4.6"
    },
    "suggest": {
        "ext-curl": "For Google Recaptcha support",
        "ext-json": "For Google Recaptcha support",
        "ext-fileinfo": "For mime type upload validation"
    },
    "keywords": [
        "form",
        "form-builder",
        "bootstrap",
        "recaptcha",
        "captcha",
        "ajax"
    ]
}
